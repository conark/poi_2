<table class="table is-fullwidth">
  <thead>
    <th>Place name</th>
    <th>Rate</th>
    <th>Review</th>
    <th>Creator</th>
  </thead>
  <tbody>
    {{#each reviews}}
      <tr>
        <td>{{place.placename}}</td>
        <td>{{rate}}</td>
        <td>{{review}}</td>
        <td>{{donor.firstName}} {{donor.lastName}}</td>
        
      </tr>
    {{/each}}
  </tbody>
</table>
